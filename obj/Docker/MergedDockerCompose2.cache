services:
  customers:
    build:
      context: /Users/shark/Projects/MicroservicesSample
      dockerfile: Customers/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Customers
      target: base
    container_name: Customers
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: customers:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/Customers.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 4000
      target: 80
    - published: 4001
      target: 443
    tty: true
    volumes:
    - /Users/shark/Projects/MicroservicesSample/Customers:/app:rw
    - /Users/shark/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/shark/.aspnet/https:/root/.aspnet/https:ro
    - /Users/shark/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /usr/local/share/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /Users/shark/.nuget/packages:/root/.nuget/packages:ro
    - /Users/shark/Projects/MicroservicesSample:/src:rw
  orders:
    build:
      context: /Users/shark/Projects/MicroservicesSample
