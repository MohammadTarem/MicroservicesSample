services:
  customers:
    build:
      context: /Users/shark/Projects/MicroservicesSample
      dockerfile: Customers/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: customers
    ports:
    - target: 80
